<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <script src="jquery-3.5.1.min.js"></script>

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">


    <title>Surprise My Palate - Go Explore the Food World!</title>

<style>
    body {
        background-image: url("assets/japanesefoodwallpaper.jpg");
        background-repeat: no-repeat;
        background-attachment: fixed;
        background-size:100% 100%;
    }
</style>

</head>
<body>

    <div class="container">
        <div class="container">
            <div class="row">

            </div>
            <form>
                <div class="form-group">
                    <input type="text" class="form-control" id="ingredient">
                </div>
                <input type="button" value="Add ingredient to blacklist"
                    onclick="addBlacklist(document.getElementById('ingredient').value)">
                <p id="currentBlacklist"></p>
                <input type="button" value="Submit blacklist" onclick="submitBlacklist()">
                <!--<input type="button" id="submitBlacklistButton" value="Submit blacklist">-->

            </form>
            <div class="row">
                <div class="col-sm">
                    <div class="card">
                        <img class="card-img-top" style="max-height:300px"
                            src="https://static01.nyt.com/images/2019/09/18/dining/13Hungry/13Hungry-superJumbo-v2.jpg">
                        <div class="card-body">
                            <h5 class="card-title">Nurlan Uyghur Restaurant</h5>
                            <p class="card-text">Halal, Uzbek, Asian Fusion</p>
                            <p class="card-text">4.5 stars, 51 reviews</p>
                            <a href="https://www.yelp.com/biz/nurlan-uyghur-restaurant-flushing-2" class"btn
                                btn-primary">See on
                                Yelp</a>
                        </div>
                    </div>
                </div>

                <div class="col-sm">
                    <div class="card">
                        <img class="card-img-top" style="max-height:300px"
                            src="https://s3-media0.fl.yelpcdn.com/bphoto/Sm89jMORUKw8o81TRuwWgw/o.jpg">
                        <div class="card-body">
                            <h5 class="card-title">Asian Taste 86</h5>
                            <p class="card-text">Halal, Indonesian</p>
                            <p class="card-text">4 stars, 97 reviews</p>
                            <a href="https://www.yelp.com/biz/asian-taste-86-elmhurst" class"btn btn-primary">See on
                                Yelp</a>
                        </div>
                    </div>
                </div>

                <div class="col-sm">
                    <div class="card" style="width: 18rem;">
                        <img class="card-img-top" style="max-height:300px"
                            src="https://s3-media0.fl.yelpcdn.com/bphoto/dMFNVpCKvrAc1qagaIH_Ow/o.jpg">
                        <div class="card-body">
                            <h5 class="card-title">Kabul Kabab Restaurant</h5>
                            <p class="card-text">Afghan, Halal</p>
                            <p class="card-text">4 stars, 482 reviews</p>
                            <a href="https://www.yelp.com/biz/kabul-kabab-house-flushing" class"btn btn-primary">See on
                                Yelp</a>
                        </div>
                    </div>

                </div>

            </div>

        </div>

    </div>

    <script src="javascript/blacklist.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.js"></script>
    </script>

</body>

</html>